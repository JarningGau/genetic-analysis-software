    <item>
      <title>GAP</title>
      <link>https://gaow.github.io/genetic-analysis-software/g/gap/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      
      <guid>https://gaow.github.io/genetic-analysis-software/g/gap/</guid>
